@model List<Medilearn.Data.Entities.Course>
@{
    Layout = "_LayoutPersonnel";
    ViewBag.Title = "Kurslarım";
}
<h2>Kayıtlı Kurslarım</h2>

@foreach (var course in Model)
{
    <div class="card mb-3">
        <div class="card-body">
            <h5>@course.Title</h5>
            <p>@course.Description</p>
            <a asp-action="CourseDetails" asp-route-id="@course.Id" class="btn btn-success">Başla</a>
        </div>
    </div>
}
